#!/usr/bin/env osh
:
: osh - w/ shell sequence + sh6 external 'goto' is goto
:

( \
   echo -n 'if "' ; \
   fd2 -f/dev/null which goto | tr -d '\n' ; \
   echo -n '" = "' ; \
   strings ../sh6 | grep goto | tr -d '\n' ; \
   echo -n '" ' ; \
   echo "if { echo Notice below that \'goto\' is goto. } exit" ; \
   echo 'fd2 -e echo error ; false' \
) >gig-$$

: fd2 -e cat ./gig-$$ ; source ./gig-$$ ; rm -f gig-$$

echo 1
'goto' label
echo 2
: label
echo 3
