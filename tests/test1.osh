: Pipes...

	echo Hello! | cat | grep '^H'
	echo Hello! ^ cat ^ grep '^H'
	echo Hello! | : nothing | grep '^H'
	echo Hello! ^ : nothing ^ grep '^H'
	echo Hello!|cat|grep '^H'
	echo Hello!^cat^grep '^H'
	echo Hello!|: nothing|grep '^H'
	echo Hello!^: nothing^grep '^H'

: Subshells...

	( echo one ); ( echo two ); ( echo three )
	( ( echo one ); ( echo two ); ( echo three ) )
	( echo \( one \) ); ( echo \( two \) ); ( echo \( three \) )
	( ( echo \( one \) ); ( echo \( two \) ); ( echo \( three \) ) )
	( echo Hello! ) | ( cat ) | ( grep '^H' )
	( echo Hello! ) ^ ( cat ) ^ ( grep '^H' )
	( echo Hello! ) | ( cat >/dev/null ) | ( grep '^H' )
	( echo Hello! ) ^ ( cat >/dev/null ) ^ ( grep '^H' )
	( echo Hello! ) | ( : nothing ) | ( grep '^H' )
	( echo Hello! ) ^ ( : nothing ) ^ ( grep '^H' )

	(echo one);(echo two);(echo three)
	((echo one);(echo two);(echo three))
	(echo \(one\));(echo \(two\));(echo \(three\))
	((echo \(one\));(echo \(two\));(echo \(three\)))
	(echo Hello!)|(cat)|(grep '^H')
	(echo Hello!)^(cat)^(grep '^H')
	(echo Hello!)|(cat >/dev/null)|(grep '^H')
	(echo Hello!)^(cat >/dev/null)^(grep '^H')
	(echo Hello!)|(: nothing)|(grep '^H')
	(echo Hello!)^(: nothing)^(grep '^H')

	( ( echo Hello! ) | \
	  ( cat | ( cat | cat | cat ) | cat ) | \
	  ( grep '^H' ) )
	( ( echo Hello! ) ^ \
	  ( cat ^ ( cat ^ cat ^ cat ) ^ cat ) ^ \
	  ( grep '^H' ) )
	( ( echo Hello! ) | \
	  ( cat | ( cat | : nothing | cat ) | cat ) | \
	  ( grep '^H' ) )
	( ( echo Hello! ) ^ \
	  ( cat ^ ( cat ^ : nothing ^ cat ) ^ cat ) ^ \
	  ( grep '^H' ) )

	((echo Hello!)|(cat|(cat|cat|cat)|cat)|(grep '^H'))
	((echo Hello!)^(cat^(cat^cat^cat)^cat)^(grep '^H'))
	((echo Hello!)|(cat|(cat|: nothing|cat)|cat)|(grep '^H'))
	((echo Hello!)^(cat^(cat^: nothing^cat)^cat)^(grep '^H'))

